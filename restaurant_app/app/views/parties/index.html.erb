<h1>Party Index</h1>

<h6><%= link_to "Add a new party", new_party_path %></h6>
<h6><%= link_to "Home", profile_path %></h6>

<% @parties.each do |party| %>

  <p>Table Number: <%= link_to( party.table_num, party_path(party.id) ) %></p>
  <p>Number of Guests: <%= party.num_of_guests %> </p>
  <p>Is paid? <%= party.is_paid %></p>
  <h6><%= link_to "View Orders", party_orders_path(party.id)%></h6>
  <h6><%= link_to "Add Order", new_party_order_path(party.id) %></h6>
<% end %>
