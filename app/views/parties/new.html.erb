<h4>NEW TABLE</h4>

<h6><%= link_to "OPEN TABLES", parties_path, :class => 'links' %><%= link_to "ALL TABLES", all_tables_path, :class => 'links' %></h6>

<%= form_for @party do |f| %>

  <%= f.label :table_num, "Table Number" %>
  <%= f.text_field :table_num %>

  <%= f.label :num_of_guests, "Number of Guests" %>
  <%= f.text_field :num_of_guests %>

  <br>
  <%= f.submit %>
<% end %>
